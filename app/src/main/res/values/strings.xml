    <xliff:g id="bullet">\u25AA</xliff:g> <xliff:g id="forum">%s</xliff:g>
  </string>

  <!-- FORUMS -->
  <string name="forum_comments_template">
   Comments: <xliff:g id="answers">%s</xliff:g>
  </string>
  <string name="forum_pinned_topic_template" translatable="false">
    <xliff:g id="icon">{cmd-pin}</xliff:g> <xliff:g id="title">%s</xliff:g>
  </string>

  <!-- POSTS -->
  <string name="post_hidden_text_template">Hidden text: <xliff:g id="spoiler">%s</xliff:g></string>
  <string name="post_link">Link</string>

  <!-- NAVIGATION PANEL -->
  <string name="navigation_pages_template">
    Page <xliff:g id="page">%d</xliff:g> of <xliff:g id="total_pages">%d</xliff:g>
  </string>
  <string name="navigation_previous">Back</string>
  <string name="navigation_next">Next</string>
