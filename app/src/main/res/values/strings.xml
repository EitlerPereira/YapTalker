  <!-- MENUS -->
  <string name="action_save">Save</string>
  <string name="action_share">Share</string>

  <!-- SHORT DATE STRINGS -->
  <plurals name="short_date_years">
    <item quantity="one"><xliff:g id="years">%d</xliff:g> year(s)</item>
    <item quantity="few"><xliff:g id="years">%d</xliff:g> year(s)</item>
    <item quantity="many"><xliff:g id="years">%d</xliff:g> year(s)</item>
    <item quantity="other"><xliff:g id="years">%d</xliff:g> year(s)</item>
  </plurals>

  <string name="short_date_month" tools:ignore="PluralsCandidate">
    <xliff:g id="month">%d</xliff:g> month(s)
  </string>

  <plurals name="short_date_days">
    <item quantity="one"><xliff:g id="days">%d</xliff:g> day(s)</item>
    <item quantity="few"><xliff:g id="days">%d</xliff:g> day(s)</item>
    <item quantity="many"><xliff:g id="years">%d</xliff:g> day(s)</item>
    <item quantity="other"><xliff:g id="days">%d</xliff:g> day(s)</item>
  </plurals>

  <plurals name="short_date_hours">
    <item quantity="one"><xliff:g id="days">%d</xliff:g> hour(s)</item>
    <item quantity="few"><xliff:g id="days">%d</xliff:g> hour(s)</item>
    <item quantity="many"><xliff:g id="years">%d</xliff:g> hour(s)</item>
    <item quantity="other"><xliff:g id="days">%d</xliff:g> hour(s)</item>
  </plurals>

  <string name="short_date_minutes"><xliff:g id="minutes">%d</xliff:g> min(s)</string>
  <string name="short_date_seconds_now">just now</string>

